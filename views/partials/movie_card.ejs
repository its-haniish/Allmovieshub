<a href="/<%= movie.slug %>" class="movie_card">
  <figure>
    <img
      src="https://allmovieshub.live/images/poster/<%= movie.slug %>.jpg"
      alt="<%= movie.title %>"
      loading="lazy"
      onerror="handleImageError(this)"
    />
  </figure>
  <figcaption>
    <%= movie.title %>
  </figcaption>
</a>
